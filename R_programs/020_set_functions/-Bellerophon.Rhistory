discretise<- function(x,n){
if(mode(x)!="numeric"|mode(n)!="numeric"|n<=0)
return(NULL)
n<- as.integer(n)
xd<- vector(length=length(x))
xmin<- x[1]
xmax<- x[1]
for(i in seq_along(x)){
if(x[i]<xmin)
xmin<- x[i]
if(x[i]>xmax)
xmax<- x[i]
}
width<- (xmax-xmin)/n
for(i in seq_along(x)){
if(x[i]==xmax)
xd[i]<- xmax-0.5*width
else(xd[i]<- xmin+ (as.integer((x[i]-xmin)/width)+0.5)*width)
}
print(xd)
}
x<- as.vector(unlist(read.csv("numbers.csv", header=FALSE)))
xd<- discretise(x,3)
print(xd)
# VECTORIZED VERSION:
# uses:
# - same elements as before, adding xd.tmp to manage the case in which the element of x considered is maximum (xd.tmp stores the distretised values of x)
x<- as.vector(unlist(read.csv("numbers.csv", header=FALSE)))
n<- 3
if(mode(x)!="numeric"|mode(n)!="numeric"|n<=0)
return(NULL)
xmin<- min(x)
xmax<- max(x)
width<- (xmax-xmin)/n
xd<- xmin+ (as.integer((x-xmin)/width)+0.5)*width
xd.tmp<- xd
xd.tmp[which(xd.tmp==max(xd.tmp))]<- 0
xd.tmp[xd.tmp==0]<- xd.tmp[max(xd.tmp)]
print(xd.tmp)
